            in += "0";
            eti1.setText(in);
            obtenerdinero();
        }
    }                                       

    private void boton1ActionPerformed(java.awt.event.ActionEvent evt) {                                       
        in = "";
        eti1.setText("0");
        punto = true;
    }                                      

    private void boton13ActionPerformed(java.awt.event.ActionEvent evt) {                                        
        if (punto) {
            if (in == "") {
                in = "0.";
                eti1.setText(in);
            } else {
                in += ".";
                eti1.setText(in);
            }
        }
        punto = false;
    }                                       

