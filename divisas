    private void obtenerdinero() {
        dinero = Double.parseDouble(in);
        cambiodivisas();
        dinero *= cambio;
        eti2.setText(String.format("%.2f", dinero));
    }

    private void cambiodivisas() {
        if (divisastex.equals(divisastex1)) {
            cambio = 1;
        } else if (divisastex.equals("Estados Unidos-Dolar") && divisastex1.equals("Perú-Sol")) {
            cambio = 3.89;
        } else if (divisastex.equals("Estados Unidos-Dolar") && divisastex1.equals("Europa-Euro")) {
            cambio = 0.82;
        } else if (divisastex.equals("Perú-Sol") && divisastex1.equals("Estados Unidos-Dolar")) {
            cambio = 0.26;
        } else if (divisastex.equals("Perú-Sol") && divisastex1.equals("Europa-Euro")) {
            cambio = 0.21;
        } else if (divisastex.equals("Europa-Euro") && divisastex1.equals("Estados Unidos-Dolar")) {
            cambio = 1.21;
        } else if (divisastex.equals("Europa-Euro") && divisastex1.equals("Perú-Sol")) {
            cambio = 4.72;
        }
    }
    private void divisasActionPerformed(java.awt.event.ActionEvent evt) {                                        

    }                                       

    private void calculadoraActionPerformed(java.awt.event.ActionEvent evt) {                                            
        calculadora cal = new calculadora();
        cal.setVisible(true);
        dispose();
    }                                           

    private void boton2ActionPerformed(java.awt.event.ActionEvent evt) {                                       
        int dato = in.length();
        if (dato > 0) {
            if (dato == 1) {
                in = "0";
                eti1.setText(in);
            }
            in = in.substring(0, in.length() - 1);
            eti1.setText(in);
        }
        if (in == "") {
            eti1.setText("0");
            punto = true;
            eti2.setText("0");
        }
        if (in != "") {
            obtenerdinero();
        }
    }                                      

    private void combo1ActionPerformed(java.awt.event.ActionEvent evt) {                                       
        divisastex = (String) combo1.getSelectedItem();
        if (divisastex.equals("Estados Unidos-Dolar")) {
            simbolo1.setText("$");

        } else if (divisastex.equals("Perú-Sol")) {
            simbolo1.setText("S/");

        } else if (divisastex.equals("Europa-Euro")) {
            simbolo1.setText("Є");

        }
        if (in != "") {
            obtenerdinero();
        }

    }                                      

    private void combo2ActionPerformed(java.awt.event.ActionEvent evt) {                                       
        divisastex1 = (String) combo2.getSelectedItem();
        if (divisastex1.equals("Estados Unidos-Dolar")) {
            simbolo2.setText("$");

        } else if (divisastex1.equals("Perú-Sol")) {
            simbolo2.setText("S/");

        } else if (divisastex1.equals("Europa-Euro")) {
            simbolo2.setText("Є");

        }
        if (in != "") {
            obtenerdinero();
        }
    }                                      

    private void boton9ActionPerformed(java.awt.event.ActionEvent evt) {                                       
        in += "1";
        eti1.setText(in);
        obtenerdinero();
    }                                      

    private void boton10ActionPerformed(java.awt.event.ActionEvent evt) {                                        
        in += "2";
        eti1.setText(in);
        obtenerdinero();
    }                                       

